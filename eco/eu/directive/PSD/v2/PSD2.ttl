    rdfs:isDefinedBy <https://www.bafin.de/> .

<https://www.bafin.de/data/psd/entity/aisp/PSDDE-BAFIN-12345670>
    a                          x-psd2:AISP ;
    x-psd2:authorisationNumber "PSDDE-BAFIN-12345670"^^x-psd2:id-SemanticsId-authorisationNumber ;
    rdfs:isDefinedBy           <https://www.bafin.de/> .

