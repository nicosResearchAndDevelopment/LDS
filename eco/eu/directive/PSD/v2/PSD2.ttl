## region EBA

x-psd2:EBA
    a                            lds:Authority ;              ## REM: this **IS** an instance!!!
    rdfs:label                   "European Banking Authority"@en ;
    ## TODO: >>> PSD2 REGISTER / CREDIT INSTITUTION REGISTER
    trust:trustedParticipantList x-psd2:EBA-data-trustedParticipantList ;
    x-psd2:ncaList               x-psd2:EBA-data-data-ncaList ;
    trust:listOfTrustedLists     [ a          trust:ListOfTrustedLists ;
                                   lds:member <https://www.bafin.de/data/psd/list/aisp/> ;
                                   lds:member <https://acpr.banque-france.fr/data/psd/list/aisp/> ; ] ;
    rdfs:isDefinedBy             x-psd2: .

<did:eba:EBA>
    owl:sameAs       x-psd2:EBA ;
    rdfs:isDefinedBy x-psd2:EBA .

