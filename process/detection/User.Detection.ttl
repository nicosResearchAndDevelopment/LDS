@prefix lds:           <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix prov:          <http://www.w3.org/ns/prov#> .
@prefix rdf:           <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:          <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:           <http://www.w3.org/2001/XMLSchema#> .

@prefix process:       <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/process/> .

@prefix UserDetection: <https://github.com/nicosResearchAndDevelopment/LDS/process/detection/user#> .

UserDetection:
    a                process:Detection ;
    ## region profile
    ## endregion profile
    lds:subProcess   UserDetection:CarrierDetection ;
    lds:subProcess   UserDetection:Classification ;
    lds:subProcess   UserDetection:Identification ;
    lds:subProcess   UserDetection:Verification ;
    lds:subProcess   UserDetection:Authentication ;
    rdfs:isDefinedBy UserDetection: .

UserDetection:CarrierDetection
    a                process:Detection ;
    ## region profile
    ## endregion profile
    rdfs:comment     """Result: token.""" ;
    rdfs:isDefinedBy UserDetection: .

UserDetection:Classification
    a                process:Classification ;
    ## region profile
    ## endregion profile
    rdfs:comment     """Result: classified **AND** Token.""" ;
    rdfs:isDefinedBy UserDetection: .

UserDetection:Identification
    a                process:Identification ;
    ## region profile
    ## endregion profile
    rdfs:isDefinedBy UserDetection: .

UserDetection:Verification
    a                process:Verification ;
    ## region profile
    ## endregion profile
    rdfs:isDefinedBy UserDetection: .

UserDetection:Authentication
    a                process:Authentication ;
    ## region profile
    ## endregion profile
    rdfs:isDefinedBy UserDetection: .