@prefix lds:            <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .

lds:Legal
    a                owl:Class, skos:Concept ;
    skos:notation    "Legal"^^xsd:string ;
    skos:prefLabel   "Legal"@en ;
    skos:prefLabel   "Legal"@de ;
    skos:narrower    lds:LegalEntity ;
    rdfs:isDefinedBy lds: .

lds:legalBasis
    a                rdf:Property ;
    skos:notation    "legalBasis"^^xsd:string ;
    skos:prefLabel   "Legal Basis"@en ;
    skos:prefLabel   "Legale Basis"@de ;
    rdfs:domain      lds:Legal ;
#    rdfs:range       ILO (intentional left open) ;
    rdfs:isDefinedBy lds: .

lds:LegalConcepts
    skos:member lds:Legal .

lds:AllConcepts
    skos:member lds:Legal .