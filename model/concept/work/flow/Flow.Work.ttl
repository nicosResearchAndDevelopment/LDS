@prefix lds:                    <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix prov:                   <http://www.w3.org/ns/prov#> .
@prefix rdf:                    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:                   <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:                    <http://www.w3.org/2001/XMLSchema#> .

@prefix work:                   <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/work/> .
@prefix flow:                   <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/work/flow/> .

flow:WorkFlow
    a                owl:Class, skos:Concept ;
    rdfs:subClassOf  work:Work ;
    rdfs:subClassOf  flow:Flow ;
    skos:notation    "flow:WorkFlow"^^xsd:string ;
    skos:prefLabel   "Workflow"@en ;
    skos:prefLabel   "Arbeitsfluss"@de ;
    skos:broader     flow:Flow ;
    skos:broader     lds:Work ;
    rdfs:isDefinedBy lds: .

lds:WorkConcepts
    skos:member flow:WorkFlow .

lds:AllConcepts
    skos:member flow:WorkFlow .