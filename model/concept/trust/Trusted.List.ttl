@prefix lds:                <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix owl:                <http://www.w3.org/2002/07/owl#> .
@prefix prov:               <http://www.w3.org/ns/prov#> .
@prefix rdf:                <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:               <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:               <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:                <http://www.w3.org/2001/XMLSchema#> .

@prefix trust:              <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/trust/> .

trust:TrustedList
    a                owl:Class, skos:Concept ;
    rdfs:subClassOf  lds:Trust ;
    rdfs:subClassOf  lds:List ;
    skos:notation    "trust:TrustList"^^xsd:string ;
    skos:prefLabel   "Trusted Entities"@en ;
    skos:prefLabel   "Vertrauenswürdige Entitäten"@de ;
    skos:broader     lds:Trust ;
    skos:broader     lds:List ;
    rdfs:isDefinedBy lds: .

lds:hasMember
    rdfs:range       rdfs:Resource ;
    rdfs:isDefinedBy lds: .

trust:trustedList
    a                owl:ObjectProperty ;
    rdfs:range       trust:TrustedList ;
    rdfs:isDefinedBy lds: .

lds:TrustConcepts
    skos:member trust:TrustList .

lds:AllConcepts
    skos:member trust:TrustList .
