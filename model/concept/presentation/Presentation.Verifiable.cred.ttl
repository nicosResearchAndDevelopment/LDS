@prefix cred:                    <https://w3.org/2018/credentials#> .
@prefix rdfs:                    <http://www.w3.org/2000/01/rdf-schema#> .

@prefix presentation:            <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/presentation/> .

presentation:VerifiablePresentation
    rdfs:subClassOf cred:VerifiablePresentation .

presentation:Object
    rdfs:subPropertyOf cred:VerifiableCredential .

presentation:verifiableObject
    rdfs:subPropertyOf cred:verifiableCredential .