@prefix lds:               <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix owl:               <http://www.w3.org/2002/07/owl#> .
@prefix prov:              <http://www.w3.org/ns/prov#> .
@prefix rdf:               <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:              <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:              <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:               <http://www.w3.org/2001/XMLSchema#> .

@prefix entity:            <https://github.com/nicosResearchAndDevelopment/LDS/model/concept/entity/> .

entity:Organisation
    a                owl:Class, skos:Concept ;
    rdfs:subClassOf  entity:LegalEntity ;
    skos:notation    "entity:Organisation"^^xsd:string ;
    skos:prefLabel   "Organisation"@en ;
    skos:prefLabel   "Organisation"@de ;
    skos:broader     entity:LegalEntity ;
    rdfs:isDefinedBy lds: .

lds:EntityConcepts
    skos:member entity:Organisation .

lds:AllConcepts
    skos:member entity:Organisation .