@prefix lds:            <https://github.com/nicosResearchAndDevelopment/LDS/> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix prov:           <http://www.w3.org/ns/prov#> .
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .

lds:LegalEntityRepresentative
    a                owl:Class, skos:Concept ;
    ## TODO : SHACL
    rdfs:subClassOf  lds:Entity ;
    rdfs:subClassOf  lds:Legal ;
    rdfs:subClassOf  lds:Representative ;
    skos:notation    "LegalEntityRepresentative"^^xsd:string ;
    skos:prefLabel   "Legal Entity Representative"@en ;
    skos:prefLabel   "Repräsentant der legalen Entität"@de ;
    skos:broader     lds:Entity ;
    skos:broader     lds:Legal ;
    skos:broader     lds:Representative ;
    rdfs:isDefinedBy lds: .

lds:EntityConcepts
    skos:member lds:LegalEntityRepresentative .

lds:LegalConcepts
    skos:member lds:LegalEntityRepresentative .

lds:RepresentativeConcepts
    skos:member lds:Representative .

lds:AllConcepts
    skos:member lds:LegalEntityRepresentative .