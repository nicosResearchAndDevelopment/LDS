## endregion upper concepts

@prefix lds:            <https://github.com/nicosResearchAndDevelopment/LDS#> .

lds:Service
    a                owl:Class ;
    rdfs:label       "Service"@en ;
    rdfs:label       "Service"@de ;
    dct:description  """Concept Schema of given Service-Family."""@en ;
    ## region skos
    a                skos:ConceptScheme ;
    skos:notation    "Service"^^xsd:string ;
    skos:prefLabel   "Service"^^xsd:string ;
    ## endregion skos
    rdfs:isDefinedBy lds: .

lds:ServiceConcept                          ## REM: abstract
    a                owl:Class ;
    ## region skos
    a                skos:Concept ;
#    skos:notation    "ServiceConcept"^^xsd:string ;
#    skos:prefLabel   "Service Concept"^^xsd:string ;
#    skos:inScheme    lds:Service ;
    ## endregion skos
    rdfs:label       "Service Concept"@en ;
    rdfs:label       "Service Konzept"@de ;
    dct:description  """."""@en ;
    rdfs:isDefinedBy lds: .

lds:ServiceProfile                          ## REM: abstract
    a                owl:Class ;
    a                skos:Concept ;
    ## region skos
    rdfs:subClassOf  prof:Profile ;
#    skos:notation    "ServiceProfile"^^xsd:string ;
#    skos:prefLabel   "ServiceProfile"^^xsd:string ;
#    skos:inScheme    lds:Service ;
    ## endregion skos
    rdfs:label       "Service Profile"@en ;
    rdfs:label       "Service-Profil"@de ;
    rdfs:comment     """An individual (so: instance) of 'lds:ServiceProfile', leading to some kind of Service-SelfDescription. Given individual is **NOT** a Service Instance (lds:ServiceInstance')!"""@en ;
    rdfs:isDefinedBy lds: .

lds:concept
    a                owl:ObjectProperty ;
    skos:notation    "concept"^^xsd:string ;
    skos:prefLabel   "Concept"^^xsd:string ;
    rdfs:label       "Profil Concept"@en ;
    dct:description  """Property to bind a given SKOS Concept."""@en ;
    rdfs:domain      lds:ServiceProfile ;
    rdfs:range       skos:Concept ;         ## REM: cardinality [1..n]
    rdfs:isDefinedBy lds: .

lds:serviceProfile
    a                owl:ObjectProperty ;
    skos:notation    "serviceProfile"^^xsd:string ;
    skos:prefLabel   "Service Profile"^^xsd:string ;
    rdfs:label       "Service-Profil"@en ;
    dct:description  """Property to bind a given Service Profile, 'lds:ServiceProfile'."""@en ;
    rdfs:range       lds:ServiceProfile ;
    rdfs:isDefinedBy lds: .

## EOF lds.service.ttl