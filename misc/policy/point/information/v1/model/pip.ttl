@prefix pip:                      <https://github.com/nicosResearchAndDevelopment/LDS/blob/main/misc/policy/information/point/> .

pip:
    a                             voaf:Vocabulary ;
    a                             owl:Ontology ;
    skos:prefLabel                "The Policy Information Point Ontology, version 1"^^xsd:string ;
    rdfs:label                    "Policy Information Point Ontology (v1)"@en ;
    dct:title                     "Model for Policy Information Point"@en ;
    dct:description               """."""@en ;
    dct:creator                   <https:jottomat.solidweb.org/profile/card#me> ;
    dct:creator                   <https:jottomat.solidweb.org/profile/card#me> ;
    dct:publisher                 <https:jottomat.solidweb.org/profile/card#me> ;
    dct:created                   "2023-07-03"^^xsd:date ;
    dct:modified                  "2023-07-13"^^xsd:date ;
    owl:versionInfo               "0.0.1" ;
    vann:preferredNamespaceUri    "https://github.com/nicosResearchAndDevelopment/LDS/blob/main/misc/policy/information/point/" ;
    vann:preferredNamespacePrefix "pip:" ;
    void:vocabulary               vann:, void:, voaf:, dct: ;
    rdfs:comment                  """"""@en ;
    rdfs:isDefinedBy              <https:jottomat.solidweb.org/profile/card#me> ; .

pip:Information
    a                owl:Class ;
    rdfs:subClassOf  prov:Entity ;
    a                skos:Concept ;
    rdfs:isDefinedBy pip: .


pip:value
    a                rdf:Property ;
    rdfs:domain      pip:Information ;
    ## TODO: TBC: 'rdfs:range       rdfs:Resource' :: make it more crisp?!?
    rdfs:range       rdfs:Resource ;    ## REM: cardinality [0..n]
    rdfs:isDefinedBy pip: .

pip:unit
    a                owl:ObjectProperty ;
    rdfs:domain      pip:Information ;
    ## REM: intentionally left open - **OR**
    rdfs:range       rdfs:Resource ;
    ## REM: ... **OR**
#    rdfs:range       <http://qudt.org/schema/qudt/DerivedUnit> ;    ## REM: cardinality [0..1]
#    rdfs:range       <http://qudt.org/schema/qudt/Unit> ;
    rdfs:isDefinedBy pip: .

pip:unit
    a                owl:ObjectProperty ;
    rdfs:domain      pip:Information ;
    ## REM: intentionally left open - **OR**
    rdfs:range       rdfs:Resource ;
    ## REM: ... **OR**
#    rdfs:range       <http://qudt.org/schema/qudt/DerivedUnit> ;    ## REM: cardinality [0..1]
#    rdfs:range       <http://qudt.org/schema/qudt/Unit> ;
    rdfs:isDefinedBy pip: .


pip:validFrom
    a                owl:DatatypeProperty ;
    rdfs:domain      pip:Information ;
    rdfs:range       xsd:dateTime ;
    rdfs:isDefinedBy pip: .

pip:validUntil
    a                owl:DatatypeProperty ;
    rdfs:domain      pip:Information ;
    rdfs:range       xsd:dateTime ;
    rdfs:isDefinedBy pip: .

pip:InformationGenerationProcess
    a                owl:Class ;
    rdfs:subClassOf  prov:Activity ;
    a                skos:Concept ;
    rdfs:isDefinedBy pip: .

